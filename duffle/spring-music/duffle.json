{
    "name": "spring-music",
    "version": "0.1.0",
    "description": "Spring Music.",
    "maintainers": [
      {
          "name": "Brian Redmond",
          "email": "brianisrunning@gmail.com"
      },
      {
          "name": "Joey Schluchter",
          "email": "joey@gmail.com"
      }
  ],
    "invocationImages": {
        "cnab": {
            "name": "cnab",
            "builder": "docker",
            "configuration": {
                "registry": "cnabquickstartstest.azurecr.io/duffle"
            }
        }
    },
    "parameters": {
      "cluster_name": {
        "defaultValue": "spring-music-demo",
        "type": "string",
        "destination": {
          "env": "CLUSTER_NAME"
        }
      },
      "aks_resource_group": {
        "defaultValue": "spring-music-duffle",
        "type": "string",
        "destination": {
          "env": "AKS_RESOURCE_GROUP"
        }
      },
      "app_resource_group": {
        "defaultValue": "spring-music-duffle",
        "type": "string",
        "destination": {
          "env": "APP_RESOURCE_GROUP"
        }
      },
      "cosmosdb_service_name": {
        "defaultValue": "springmusicduffle",
        "type": "string",
        "destination": {
          "env": "COSMOSDB_SERVICE_NAME"
        }
      },
      "cosmosdb_location": {
        "defaultValue": "westeurope",
        "type": "string",
        "destination": {
          "env": "LOCATION"
        }
      }
    },
    "credentials": {
      "client_id":
        {"env":"AZURE_CLIENT_ID"},
      "client_secret":
        {"env":"AZURE_CLIENT_SECRET"},
      "subscription_id":
        {"env":"AZURE_SUBSCRIPTION_ID"},
      "tenant_id":
        {"env":"AZURE_TENANT_ID"}
    }
}